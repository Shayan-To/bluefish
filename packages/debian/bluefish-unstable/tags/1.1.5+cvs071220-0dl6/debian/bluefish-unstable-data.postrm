#!/bin/sh
# postrm script for bluefish-unstable-data
#
# see: dh_installdeb(1)

set -e

if [ "$1" = remove ]; then
	dpkg-divert --package bluefish-unstable-data --remove --rename --divert \
	            /usr/share/pixmaps/gnome-mime-application-bluefish-project.stable.png \
	            /usr/share/pixmaps/gnome-mime-application-bluefish-project.png
fi

#DEBHELPER#

exit 0
